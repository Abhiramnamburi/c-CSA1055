#include<iostream>
using namespace std;
int main()
{
  int n,i;
  int a[10];
  int count=0;
  int k;
  cin>>n;
  cout<<"no of element in the array is"<<n<<"\n";
  for(i=0;i<n;i++)
  {
    cin>>a[i];
  }
  cout<<"elements in array is"<<"\n";
  for(i=0;i<n;i++)
  {
    cout<<a[i]<<"\t";
  }
  cin>>k;
  cout<<"element to find occurance is"<<k<<"\n";
  for(i=0;i<n;i++)
  {
    if(k==a[i])
      count++;
  }
  cout<<"\n"<<"occurance if the given value"<<count;
  return 0;
  
}
108.
#include<iostream>
using namespace std;
void mergeArrays(int arr1[],int arr2[],int n1,int n2,int arr3[])
{
  int i=0,j=0,k=0;
  while(i<n1&&j<n2)
  {
    if(arr1[i]<arr2[j])
    {
      arr3[k]=arr1[i];
        i++;
    }else
    {
      arr3[k]=arr2[j];
        j++;
    }
    k++;
  }
  while(i<n1)
  {
    arr3[k]=arr1[i];
    i++;
    k++;
  }
  while(j<n2)
  {
    arr3[k]=arr2[j];
    j++;
    k++;
  
  }
}
int main()
{
  int arr1[]={1,3,5,7};
  int arr2[]={2,4,6,8};
  int n1=sizeof(arr1)/sizeof(arr1[0]);
  int n2=sizeof(arr2)/sizeof(arr2[0]);
  int arr3[n1+n2];
  mergeArrays(arr1,arr2,n1,n2,arr3);
  cout<<"the merged array is:";
  for(int i=0;i<n1+n2;i++)
  {
    cout<<arr2[i]<<" ";
  }
  cout<<endl;
  return 0;
  
}

