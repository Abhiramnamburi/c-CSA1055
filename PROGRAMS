106.
#include<iostream>
using namespace std;
int main(){
  int a[100];
  int i,n;
  int max1=0,max2=0;
  cin>>n;
  cout<<"no of elements in array is:"<<n<<"\n";
  for(i=0;i<n;i++)
  {
    cin>>a[i];
  }
  cout<<"the elements in the array is:"<<n<<"\n";
  for(i=0;i<n;i++)
  {
    cout<<a[i]<<"\t";
  }
  max1=a[0];
  max2=a[1];
  for(i=0;i<n;i++)
  {
    if(a[i]>max1)
    {
      max2=max1;
      max1=a[i];
    }
    else if(a[i]>max2 && a[i]!=max1)
    {
      max2=a[i];
    }
    
  }
  cout<<"the second largest number is:"<<"\n";
  cout<<max2;
}
