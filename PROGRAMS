#include<iostream>
using namespace std;
int main()
{
  int a,num,temp,sum=0;
  cout<<"enter the value a";
  cin>>a;
  temp=a;
  while(a>0)
  {
    num=a%10;
    sum=(sum*10)+num;
    a=a/10;
  }
  if(temp==sum)
  {
    cout<<"is palindrome";
  }
  else
  {
    cout<<"is not palindrome";
  }
  return 0;
}
21.
#include<iostream>
using namespace std;
int main()
{
  int n1,n2,gcd;
  cout<<"enter two numbers:"<<endl;
  cin>>n1>>n2;
  int temp,i;
  if(n2>n1)
  {
    temp=n2;
    n2=n1;
    temp=n1;
  }
  for(i=1;i<=n2;i++){
  if(n1%i==0&&n2%i==0)
  {
    gcd=i;
  }
  }
  cout<<"gcd="<<gcd;
  return 0;
}
