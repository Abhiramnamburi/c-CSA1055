#include<iostream>
using namespace std;
int main()
{
  int n,r;
  int sum=0;
  while(n>0)
  {
    r=n%10;
    sum=sum+(r*r*r);
    n=n/10;
  }
  if(sum==n)
  {
    cout<<"amstrong number";
      
  }
  else
  {
    cout<<"not amstrong number";
  }
}
