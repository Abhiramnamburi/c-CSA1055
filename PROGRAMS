#include<iostream>
using namespace std;
bool isharshad(int n)
{
  int sum=0;
  while(n>0)
  {
    sum+=n%10;
    n/=10;
  }
  return(n%sum==0);
}
int main()
{
  int num1,num2;
  cout<<"enter two integers:";
  cin>>num1>>num2;
  if(isharshad(num1))
  {
    cout<<num1<<"is harshad number"<<endl;
  }
  else
  {
    cout<<num1<<"is not a harshad number:"<<endl;
  }
  return 0;
}
