#include<iostream>
using namespace std;
int main()
{
  int n1=22;
  int n2=33;
  int n3;
  n3=n1+n2;
  cout<<"add="<<n3<<endl;
  n3=n1-n2;
  cout<<"sub="<<n3<<endl;
  n3=n1*n2;
  cout<<"mul="<<n3<<endl;
  n3=n1/n2;
  cout<<"div="<<n3<<endl;
  n3=n1%n2;
  cout<<"modulus="<<n3<<endl;
  return 0;
    
}
