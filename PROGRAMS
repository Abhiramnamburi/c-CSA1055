int main()
{
  int num;
  cout<<"enter the number";
  cin>>num;
  int square=num*num;
  int sum=0;
  while(square>0)
  {
    int lastdigit=square%10;
    sum=sum+lastdigit;
    square=square/10;
  }
  if(sum==num)
    cout<<num<<"is neon num";
  else
    cout<<num<<"not neon num";
  return 0;
}
